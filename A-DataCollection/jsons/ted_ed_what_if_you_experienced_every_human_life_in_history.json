4 \uc804\uc5d0 \n\uc2dc\uc791\ud588\ub2e4\uace0 \uc0c1\uc0c1\ud574\ubcf4\uc138\uc694.", "time": 6961}, {"__typename": "Cue", "text": "\uc9c0\uad6c\uc758 \uccab \uc778\ub958\ub85c\uc11c \ub9d0\uc774\uc8e0.", "time": 10923}, {"__typename": "Cue", "text": "\uadf8 \ub54c\uc758 \uc5ec\ub7ec\ubd84\uc740 \uc624\ub298\ub0a0 \ubaa8\ub85c\ucf54\uc640 \uac00\uae4c\uc6b4 \n\uc544\ud504\ub9ac\uce74\uc758 \ud55c \uc9c0\uc5ed\uc5d0 \uc0b4\uba70", "time": 13342}, {"__typename": "Cue", "text": "\uc6b0\ub9ac\uac00 \uc798 \uc544\ub294 \uc778\ub958\uc758 \uc870\uc0c1\ub4e4\uacfc \n\ubcc4\ub85c \ub2e4\ub974\uc9c0 \uc54a\uc740 \uc0b6\uc744 \uc0b4\uace0 \uc788\uc744 \uac81\ub2c8\ub2e4.", "time": 16679}, {"__typename": "Cue", "text": "\uc5c9\uc131\ud55c \uc11d\uae30\ub97c \ub9cc\ub4e4\uace0, \uc0ac\ub0e5\uc744 \ud558\uace0, \n\uc74c\uc2dd\uacfc \uc7ac\ub8cc\ub4e4\uc744 \ubaa8\uc73c\uaca0\uc8e0.", "time": 20099}, {"__typename": "Cue", "text": "\ub9c8\uce68\ub0b4 \uc8fd\uc744 \ub54c\uae4c\uc9c0 \ub9d0\uc774\uc8e0.", "time": 24645}, {"__typename": "Cue", "text": "\ud558\uc9c0\ub9cc \uc774 \uac83\uc740 \uc2dc\uc791\uc5d0 \ubd88\uacfc\ud569\ub2c8\ub2e4.", "time": 28274}, {"__typename": "Cue", "text": "\uc5ec\ub7ec\ubd84\uc740 \uc774\uc81c \ub2e4\uc2dc \uc2dc\uac04 \uc5ec\ud589\uc744 \ud558\uace0 \n\ud658\uc0dd\ud560 \uac83\uc774\uae30 \ub54c\ubb38\uc774\uc8e0.", "time": 30485}, {"__typename": "Cue", "text": "\ub450 \ubc88\uc9f8 \uc778\ub958\ub85c\uc11c\uc694.", "time": 35448}, {"__typename": "Cue", "text": "\uc774\uc804\uc758 \uc0b6\uc744 \uae30\uc5b5\ud558\uc9c0 \uc54a\ub294\ub2e4\uace0 \ud574\ub3c4", "time": 37992}, {"__typename": "Cue", "text": "\uccab \ubc88\uc9f8 \uc778\ub958\ub85c\uc11c \ud55c \ud589\ub3d9\ub4e4\uc740 \n\uc5ec\ub7ec\ubd84\uc5d0\uac8c \uc601\ud5a5\uc744 \uc8fc\uac8c \ub418\uc5b4 \uc788\uc2b5\ub2c8\ub2e4.", "time": 40995}, {"__typename": "Cue", "text": "\uadf8\ub9ac\uace0 \ub2e4\uc2dc \uc8fd\uc740 \ud6c4\n\uc138 \ubc88\uc9f8 \uc0ac\ub78c\uc73c\ub85c \ud658\uc0dd\ud560 \uac83\uc785\ub2c8\ub2e4.", "time": 45291}, {"__typename": "Cue", "text": "\ub124 \ubc88\uc9f8, \ub2e4\uc12f \ubc88\uc9f8, \n\uacc4\uc18d \uadf8\ub807\uac8c \ub9d0\uc774\uc8e0.", "time": 49378}, {"__typename": "Cue", "text": "\uadf8\ub807\uac8c \uc9c0\uad6c\uc5d0 \ubc1c\uc744 \ubd99\uc774\uace0 \uc0b4\uc544\ubd24\ub358 \n\ubaa8\ub4e0 \uc0ac\ub78c\uc758 \uc0b6\uc744 \uc0b4\uc544\ubcf4\ub294 \uac70\uc608\uc694.", "time": 51589}]}, {"__typename": "Paragraph", "cues": [{"__typename": "Cue", "text": "\uc990\uc904\uc774 \uc774\uc5b4\uc9c0\ub294 \uc774 \uc0b6\uc740 \n4\uc870 \ub144\uc744 \uc774\uc5b4\uac08 \uac83\uc785\ub2c8\ub2e4.", "time": 56636}, {"__typename": "Cue", "text": "\uc9c0\uae08 \uc5ec\ub7ec\ubd84\uc774 \uc0b4\uace0 \uc788\ub294 \n\uc778\uc0dd \ub9cc\uc744 \uae30\uc5b5\ud560 \uc218 \uc788\uae30 \ub54c\ubb38\uc5d0", "time": 61432}, {"__typename": "Cue", "text": "\uc5ec\ub7ec\ubd84\uc758 \uc815\uc2e0\uc740 \ubaa8\ub4e0 \uc778\ub958\uc758 \uc5ed\uc0ac\ub97c \n\uae30\uc5b5\ud558\uace0 \uc788\uc9c0\ub294 \uc54a\uc2b5\ub2c8\ub2e4.", "time": 64936}, {"__typename": "Cue", "text": "\ud558\uc9c0\ub9cc \uac01\uae30 \ub2e4\ub978 \uc778\uc0dd\uc740 \n\uc5ec\ub7ec\ubd84\uc758 \ubbf8\ub798 \uc0b6\uc5d0 \ud070 \uc601\ud5a5\uc744 \ubbf8\uce58\uc8e0.", "time": 68648}, {"__typename": "Cue", "text": "\uc5ec\ub7ec\ubd84\uc758 \uc601\ud5a5\ub825\uc740 \ub54c\ub54c\ub85c \n\uc804 \uc138\uacc4\uc5d0 \uc5c4\uccad\ub09c \uc601\ud5a5\uc744 \ub07c\uce69\ub2c8\ub2e4.", "time": 74112}, {"__typename": "Cue", "text": "\ud558\uc9c0\ub9cc \uc774\ub7f0 \uc5ed\uc0ac\uc801\uc778 \uc778\ubb3c\ub4e4\ub3c4", "time": 76823}, {"__typename": "Cue", "text": "\uc778\uc0dd\uc758 \uc544\uc8fc \uc791\uc740 \ubd80\ubd84\uc77c \ubfd0\uc774\uc8e0.", "time": 79325}, {"__typename": "Cue", "text": "\uc624\ud788\ub824 \uc5ec\ub7ec\ubd84\uc740 \ub300\uccb4\ub85c\n\ud3c9\ubc94\ud55c \uc0b6\uc744 \uc0b4\uac8c \ub420 \uac83\uc785\ub2c8\ub2e4.", "time": 82286}, {"__typename": "Cue", "text": "\uba39\uace0, \uc6c3\uace0, \uc77c\ud558\uace0, \uac71\uc815\ud558\ub294 \n\uadf8\ub7f0 \uc77c\uc0c1\ub4e4\ub85c \ucc44\uc6cc\uc9c4 \uc0b6\uc744 \ub9d0\uc774\uc608\uc694.", "time": 86332}, {"__typename": "Cue", "text": "\uc5ec\ub7ec\ubd84\uc740 4\uc870 \ub144\uc758 \n\uc778\uc0dd\uc758 \uc2ed\ubd84\uc758 \uc77c \ub3d9\uc548", "time": 91295}, {"__typename": "Cue", "text": "\uc720\ubaa9\ubbfc\uc73c\ub85c \uc0ac\ub0e5\uc744 \ud558\uba70 \uc0b4 \uac83\uc785\ub2c8\ub2e4.", "time": 94799}, {"__typename": "Cue", "text": "60%\ub294 \ub18d\ubd80\ub85c\uc11c \ub18d\uae30\uad6c\ub97c \ub9cc\ub4e4\uace0 \n\ub18d\uc5c5 \uae30\uc220\uc744 \ubc1c\uc804\uc2dc\ud0a4\uba70", "time": 96509}, {"__typename": "Cue", "text": "8000\uc5b5 \ub144 \uac00\ub7c9\uc744 \n\ub18d\uc7a5\uc5d0\uc11c \uc77c\ud558\uba70 \uc0b4\uc544\uac00\uaca0\uc8e0.", "time": 101055}, {"__typename": "Cue", "text": "\uc774 \uc0b6 \uc18d\uc5d0\uc11c 15\uc5b5 \ub144\uc744 \n\uc131\uad00\uacc4\ub97c \ud558\ub294\ub370 \uc4f8 \uac83\uc774\uace0", "time": 106561}, {"__typename": "Cue", "text": "2\uc5b5 5\ucc9c\ub9cc \ub144 \ub3d9\uc548\uc740 \n\ucd9c\uc0b0\uc744 \ud560 \uac83\uc785\ub2c8\ub2e4.", "time": 111190}, {"__typename": "Cue", "text": "\uc804\uccb4\uc801\uc73c\ub85c 20%\uc758 \uc0b6\uc744 \uc790\uc2dd\uc744 \n\uae30\ub974\ub294\ub370 \uc0ac\uc6a9\ud560 \uac83\uc774\uc8e0.", "time": 115194}, {"__typename": "Cue", "text": "\uc544\uc774\ub4e4\uc5d0\uac8c \ub2e4\uc591\ud55c \n\ubb38\ud654\uc801 \uac00\uce58\ub4e4\uc744 \ubb3c\ub824\uc8fc\uace0", "time": 119407}, {"__typename": "Cue", "text": "\uadf8\ub4e4\uc758 \uc0b6\uc758 \ubc29\ud5a5\uc5d0 \n\uc601\ud5a5\uc744 \ubbf8\uce58\uba74\uc11c \ub9d0\uc774\uc8e0.", "time": 122034}, {"__typename": "Cue", "text": "\uc5b4\ub5a4 \uc0b6\uc5d0\uc11c \uadf8\ub7ec\ud55c \ubb38\ud654\ub4e4\uc744\n\uce68\ub7b5\uacfc \uc81c\uad6d\uc8fc\uc758\ub97c \ud1b5\ud574 \ud769\ub728\ub9b4 \uac83\uc785\ub2c8\ub2e4.", "time": 124996}, {"__typename": "Cue", "text": "\ub2e4\ub978 \uc0b6\uc5d0\uc11c\ub294 \uc5ec\ub7ec\ubd84\uc758 \ub545\uacfc\n\uc0ac\ub791\ud558\ub294 \uc0ac\ub78c\ub4e4\uc774 \ub04c\ub824\uac00\ub294 \uace0\ud1b5\uc744 \uacaa\uaca0\uc8e0.", "time": 129500}, {"__typename": "Cue", "text": "1%\uac00 \ub118\ub294 \uc0b6 \ub3d9\uc548 \ub9d0\ub77c\ub9ac\uc544\uc640 \n\ub450\ucc3d\uc5d0 \uac78\ub824 \uad34\ub85c\uc6cc\ud560 \uac83\uc774\uace0", "time": 133713}, {"__typename": "Cue", "text": "\ub2e4\ub978 \uc0b6\uc5d0\uc11c\ub294 \uc774\ub7ec\ud55c \uc9c8\ubcd1\ub4e4\uc744 \uce58\ub8cc\ud558\uba70", "time": 138509}, {"__typename": "Cue", "text": "\uc140 \uc218 \uc5c6\uc774 \ub9ce\uc740 \ub610 \ub2e4\ub978 \uc5ec\ub7ec\ubd84\uc744 \n\uad6c\ud560 \uac83\uc785\ub2c8\ub2e4.", "time": 140803}]}, {"__typename": "Paragraph", "cues": [{"__typename": "Cue", "text": "\uc778\ub958\uac00 \ub9c9 \uc2dc\uc791\ud588\uc744 \ub54c\uc758 \n\uc218\uba85\uc740 \ube44\uad50\uc801 \uc9e7\uc2b5\ub2c8\ub2e4.", "time": 144515}, {"__typename": "Cue", "text": "\uc0ac\ub294 \uc0ac\ub78c\ub3c4 \uc801\uc5c8\uace0\uc694.", "time": 148978}, {"__typename": "Cue", "text": "\uc5ec\ub7ec\ubd84\uc758 \uc601\ud5a5\ub825\ub3c4 \ubb3c\ub9ac\uc801\uc73c\ub85c \n\uac00\uae4c\uc774 \uc788\ub294 \uc0ac\ub78c\uc5d0\uac8c\ub9cc \uad6d\ud55c\ub429\ub2c8\ub2e4.", "time": 150605}, {"__typename": "Cue", "text": "\ud558\uc9c0\ub9cc \uc778\uac04\uc774 \uc624\ub798 \uc0b4\uc544\ub0a8\uace0 \n\ud3c9\uade0 \uc778\uad6c\uac00 \ub298\uc5b4\ub098\uba74\uc11c", "time": 154567}, {"__typename": "Cue", "text": "\uc11c\ub85c \ube44\uc2b7\ud558\uac8c \ud560 \uc77c\uc774 \uac00\ub4dd\ud55c \uc778\uc0dd\ub4e4\uc744 \uc0ac\ub294\ub370 \n\uc2dc\uac04\uc744 \ub9ce\uc774 \uc18c\uc694\ud558\uac8c \ub420 \uac83\uc785\ub2c8\ub2e4.", "time": 158863}, {"__typename": "Cue", "text": "\uc778\uc0dd\uc758 3\ubd84\uc758 1\uc740 \n1200\ub144 \uc774\ud6c4\uc5d0 \uc2dc\uc791\ub420 \uac83\uc774\uace0,", "time": 163451}, {"__typename": "Cue", "text": "\uadf8 \ud6c4 4\ubd84\uc758 1\uc740 1750\ub144 \uc774\ud6c4\uc5d0 \n\uc77c\uc5b4\ub098\uac8c \ub420 \uac83\uc785\ub2c8\ub2e4.", "time": 167455}, {"__typename": "Cue", "text": "\uc774 \uc2dc\uc810\uc5d0\uc11c \uae30\uc220\uacfc \uc0ac\ud68c\ub294 \n\uadf8 \uc5b4\ub290 \ub54c\ubcf4\ub2e4 \ube60\ub974\uac8c \ubcc0\ud558\uace0 \uc788\uc2b5\ub2c8\ub2e4.", "time": 171125}, {"__typename": "Cue", "text": "\uc99d\uae30 \uae30\uad00\uc744 \ubc1c\uba85\ud560 \uac83\uc774\uace0, \n\uacf5\uc7a5\uc744 \uc9d3\uae30 \uc2dc\uc791\ud560 \uac83\uc774\uba70,", "time": 175546}, {"__typename": "Cue", "text": "\ub9e4\uc77c \uc0ac\uc6a9\ud558\ub294 \uae30\uacc4\ub97c \uc791\ub3d9\uc2dc\ud0a4\uae30 \uc704\ud574 \n\uc804\ub825\uc744 \ubc1c\uc0dd\uc2dc\ud0a4\uaca0\uc8e0.", "time": 180134}, {"__typename": "Cue", "text": "\uc5ed\uc0ac \uc18d\uc5d0 \uc788\ub294 \uacfc\ud559\uc801\uc778 \ud601\uba85\uacfc \n\ubb34\uc2dc\ubb34\uc2dc\ud55c \uc804\uc7c1\uc744 \uacaa\uc744 \uac83\uc774\uace0", "time": 184388}, {"__typename": "Cue", "text": "\uadf9\uc801\uc778 \ud658\uacbd \uc624\uc5fc\uc744 \ubcf4\uac8c \ub420 \uac83\uc785\ub2c8\ub2e4.", "time": 188768}, {"__typename": "Cue", "text": "\ud3c9\uade0\uc801\uc73c\ub85c \ud658\uc0dd\uc744 \ubc18\ubcf5\ud560\uc218\ub85d \n\uc218\uba85\uc774 \ub354 \uae38\uc5b4\uc9c0\uaca0\uc9c0\ub9cc", "time": 191437}, {"__typename": "Cue", "text": "\uc0dd\ud65c\uc758 \uc18d\ub3c4\ub294 \uc810\uc810 \ube68\ub77c\uc9c8 \uac83\uc785\ub2c8\ub2e4.", "time": 194190}, {"__typename": "Cue", "text": "\uba87 \ub2ec\uc774 \uac78\ub9ac\ub358 \ub300\ud654\ub294 \n\uc774\uc81c \uba87 \ubd84\uc774\uba74 \ub098\ub20c \uc218 \uc788\uac8c \ub418\uace0", "time": 197235}, {"__typename": "Cue", "text": "\ubd84\uc5ec\ub7ec\ubd84\uc774 \uc2dc\uc791\ud55c \ubca4\ucc98 \uc0ac\uc5c5\ub4e4\uc740 \n\uc138\ub300\ub97c \uac70\uce58\uba70 \ube60\ub974\uac8c \ubc14\ub00c\uaca0\uc8e0.", "time": 202073}, {"__typename": "Cue", "text": "\uc804\uc0dd\uc5d0 \uc655\uc774\uc5c8\uc744 \ub54c\ub098 \n\uc5ec\uc655\uc774\uc5c8\uc744 \ub54c\ub3c4 \ub204\ub824\ubcf4\uc9c0 \ubabb\ud55c", "time": 207203}, {"__typename": "Cue", "text": "\uc0ac\uce58\ub4e4\uc744 \ub204\ub9ac\uba70 \uc0b4 \uac83\uc785\ub2c8\ub2e4.", "time": 211249}]}, {"__typename": "Paragraph", "cues": [{"__typename": "Cue", "text": "1000\uc5b5 \uac1c\uc758 \uc778\uc0dd\uc744 \uc0b4\uc544\uac00\ub2e4 \ubcf4\uba74", "time": 214669}, {"__typename": "Cue", "text": "\ub9c8\uce68\ub0b4 \ud604\uc7ac \uac00\uc7a5 \uc5b4\ub9b0 \uc0ac\ub78c\uc73c\ub85c \n\ud658\uc0dd\ud558\uac8c \ub420 \uac83\uc785\ub2c8\ub2e4.", "time": 218172}, {"__typename": "Cue", "text": "\ube44\ub85d \uc5ec\ub7ec\ubd84\uc774 30\ub9cc \ub144\uc774\ub098 \ub418\ub294\n\uc778\ub958\uc758 \uc5ed\uc0ac\ub97c \uc0b4\uc544\uc654\ub2e4\uace0 \ud574\ub3c4", "time": 221884}, {"__typename": "Cue", "text": "\uc624\ub298 \ud589\ub3d9\uc740 \uc5ec\ub7ec\ubd84\uc758 \uc804\uc0dd\uc758 \n99%\ubcf4\ub2e4 \ud070 \ud30c\uc7a5\uc744 \ubd88\ub7ec \uc77c\uc73c\ud0b5\ub2c8\ub2e4.", "time": 225513}, {"__typename": "Cue", "text": "\uacf5\uc911\uc5d0\uc11c\uc758 \ube60\ub978 \ube44\ud589\uc740 \n\uaca8\uc6b0 \uba87 \uc2dc\uac04 \uc548\uc5d0", "time": 231477}, {"__typename": "Cue", "text": "\uc804\uc5fc\ubcd1\uacfc \uce58\ub8cc\ubc95\uc744 \uc62e\uae30\uba70 \n\ub2e4\ub2d0 \uc218 \uc788\uac8c \ud569\ub2c8\ub2e4.", "time": 235022}, {"__typename": "Cue", "text": "\uc778\ud130\ub137\uc740 \uc5ec\ub7ec\ubd84\uc758 \uc601\ud5a5\ub825\uc744 \n\uc804 \uc138\uacc4\uc5d0 \uc804\ud30c\ud558\uace0", "time": 237775}, {"__typename": "Cue", "text": "\uc9d1\uc744 \ub5a0\ub098\uc9c0 \uc54a\uace0\ub3c4 \ub204\uad70\uac00\uc640 \n\uc5b4\ub514\uc11c\ub4e0 \ud611\ub825\ud558\uac8c \ud574\uc8fc\uc8e0.", "time": 241362}, {"__typename": "Cue", "text": "\ucd5c\uadfc \uc5ec\ub7ec\ubd84\uc740 \uc720\uc804\uc790\ub97c\n\uc0c8\ub85c \uad6c\uc131\ud560 \uc218 \uc788\ub294 \ub3c4\uad6c\ub97c \uac1c\ubc1c\ud588\uace0", "time": 246575}, {"__typename": "Cue", "text": "\uc774\ub97c \ud1b5\ud574 \uadf8\ub4e4\uc758 \ud6c4\uc190\ub4e4\uc744 \n\uc601\uad6c\uc801\uc73c\ub85c \ubc14\uafb8\uc5b4 \ub193\uc558\uc2b5\ub2c8\ub2e4.", "time": 251414}, {"__typename": "Cue", "text": "\uc774\ubc88 \uc0dd\uc5d0\uc11c \uc5b4\uca4c\uba74 \n\ub354 \ub9ce\uc740 \uae30\uc220\ub4e4\uc744 \ucc3d\uc870\ud574\ub0b4\uace0", "time": 254500}, {"__typename": "Cue", "text": "\uc140 \uc218 \uc5c6\ub294 \ud6c4\uc190\ub4e4\uc758 \uc138\uc0c1\uc744 \ub354 \uc548\uc804\ud558\uace0, \n\uce5c\uc808\ud558\uace0 \ud3c9\ub4f1\ud558\uac8c \ub9cc\ub4e4 \uc218\ub3c4 \uc788\uaca0\uc8e0.", "time": 257503}, {"__typename": "Cue", "text": "\ud558\uc9c0\ub9cc \ubd80\uc8fc\uc758\ud55c \ubc1c\uba85\uc740 \n\uc27d\uac8c \uc7ac\uc559\uc73c\ub85c \ub2e4\uac00\uc62c \uac83\uc785\ub2c8\ub2e4.", "time": 262967}, {"__typename": "Cue", "text": "\ud575 \ubb34\uae30, \uc2e4\ud5d8\uc2e4 \ub204\ucd9c, \uae30\ud6c4 \ubcc0\ud654", "time": 268097}, {"__typename": "Cue", "text": "\uadf8\ub9ac\uace0 \uc874\uc7ac\ud558\ub294 \ub610 \ub2e4\ub978 \uc704\ud611 \uc18d\uc5d0\uc11c", "time": 270933}, {"__typename": "Cue", "text": "\uc778\ub958\uc758 \uba78\ub9dd\uc744 \ucd08\ub798\ud560 \uc218\ub3c4 \uc788\ub294 \uc704\ud5d8\ub4e4\uc774 \n\uc774\ub807\uac8c \ub9ce\uc558\ub358 \uc801\uc740 \uc5c6\uc2b5\ub2c8\ub2e4.", "time": 272810}, {"__typename": "Cue", "text": "\uc774 \ube60\ub974\uace0 \uc0c1\ud638 \uc5f0\uacb0\ub41c \uc138\uacc4\uc5d0\uc11c", "time": 277398}, {"__typename": "Cue", "text": "\uc778\ub958\uc758 \ubc1c\uc804\uc744 \uc6d0\uc0c1\ud0dc\ub85c \ub3cc\ub9ac\uac70\ub098\n\ubbf8\ub798\uc758 \uac00\ub2a5\uc131\uc744 \uc5c6\uc560\ub294 \uc77c\uc740", "time": 279859}, {"__typename": "Cue", "text": "\ubb34\uc11c\uc6b8 \ub9cc\ud07c \uc27d\uac8c \ud589\ud574\uc9c8 \uc218 \uc788\uc8e0.", "time": 283487}]}, {"__typename": "Paragraph", "cues": [{"__typename": "Cue", "text": "\ubbf8\ub798\uc5d0 \ubb34\uc2a8 \uc77c\uc774 \uc77c\uc5b4\ub0a0\uc9c0 \n\uc54c \uae38\uc740 \uc5c6\uc2b5\ub2c8\ub2e4.", "time": 287325}, {"__typename": "Cue", "text": "\ud558\uc9c0\ub9cc \ud655\uc2e4\ud55c \uac83\uc740 \uc5ec\ub7ec\ubd84\uc758 \uac00\ub2a5\uc131\uc740 \n\ud55c\uacc4\uac00 \uc5c6\ub2e4\ub294 \uac83\uc785\ub2c8\ub2e4.", "time": 289744}, {"__typename": "Cue", "text": "\uadf8\ub7fc \uc774\ubc88 \uc0dd\uc744 \uc5b4\ub5bb\uac8c \ubcf4\ub0b4\uc2e4 \uac74\uac00\uc694?", "time": 293831}, {"__typename": "Cue", "text": "\ub610\ud55c \uc55e\uc73c\ub85c \uc62c \uc0b6\uc5d0\n\ub354 \ub098\uc740 \ubbf8\ub798\uc5d0 \uc0b4\uae30 \uc704\ud574\uc11c", "time": 296792}, {"__typename": "Cue", "text": "\uc5b4\ub5a4 \uc77c\uc744 \ud558\uc2e4 \uc218 \uc788\ub098\uc694?", "time": 300046}]}]}, "video": {"__typename": "Video", "id": "107459", "talkExtras": {"__typename": "TalkExtras", "footnotes": []}}}, "commentsEnabled": false, "commentsLoggedInOnly": false}, "language": "en", "messages": {}, "responseCode": 200, "__N_SSP": true}