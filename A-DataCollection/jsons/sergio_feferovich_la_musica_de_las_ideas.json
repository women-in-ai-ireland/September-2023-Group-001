{"pageProps": {"talksPageBanner": {"banner": {"title": "TP WorkLife 10/12", "slug": "tp-worklife-10-12", "bannerLocation": "talks", "displayToExistingMembers": true, "mainContentHeadline": "The podcast where science meets business", "mainContent": "From fixing meetings to finding a mentor, get new insights to help you enjoy your time at work and thrive in your career.", "buttonLabel": "Listen now", "buttonLink": "https://www.ted.com/podcasts/fixable?utm_medium=website&utm_source=talkpage&utm_campaign=podcasts&utm_content=04112023-fixabletp", "showButtonIcon": false, "backgroundColor": "#FFEFA1", "textColor": "#000000", "useBackgroundGradient": false, "imageMobileBreakpoint": {"metadata": {"tags": []}, "sys": {"space": {"sys": {"type": "Link", "linkType": "Space", "id": "ab732rbh901q"}}, "id": "4BM5OASbSYegLKRYSri2gZ", "type": "Asset", "createdAt": "2023-10-11T21:22:04.069Z", "updatedAt": "2023-10-11T21:22:04.069Z", "environment": {"sys": {"id": "master", "type": "Link", "linkType": "Environment"}}, "revision": 1, "locale": "en-US"}, "fields": {"title": "WorkLife S6 - 659 x 1052x", "description": "", "file": {"url": "//images.ctfassets.net/ab732rbh901q/4BM5OASbSYegLKRYSri2gZ/c0a7605eca821fae7eea6c0359347d83/WorkLife_S6_-_659_x_1052x.png", "details": {"size": 29801, "image": {"width": 1318, "height": 210}}, "fileName": "WorkLife S6 - 659 x 1052x.png", "contentType": "image/png"}}}, "imageTabletBreakpoint": {"metadata": {"tags": []}, "sys": {"space": {"sys": {"type": "Link", "linkType": "Space", "id": "ab732rbh901q"}}, "id": "5DcAOXZmqMRBDk1Tb4tn87", "type": "Asset", "createdAt": "2023-10-11T21:25:23.492Z", "updatedAt": "2023-10-11T21:25:23.492Z", "environment": {"sys": {"id": "master", "type": "Link", "linkType": "Environment"}}, "revision": 1, "locale": "en-US"}, "fields": {"title": "WorkLife S6 - 1019 x 1602x (1)", "description": "", "file": {"url": "//images.ctfassets.net/ab732rbh901q/5DcAOXZmqMRBDk1Tb4tn87/11640e45662ad7292d8eb536a0de3f1e/WorkLife_S6_-_1019_x_1602x__1_.png", "details": {"size": 45372, "image": {"width": 2038, "height": 320}}, "fileName": "WorkLife S6 - 1019 x 1602x (1).png", "contentType": "image/png"}}}, "imageDesktopBreakpoint": {"metadata": {"tags": []}, "sys": {"space": {"sys": {"type": "Link", "linkType": "Space", "id": "ab732rbh901q"}}, "id": "3toTf7QHhGfKy59Qcx69dk", "type": "Asset", "createdAt": "2023-10-11T21:22:04.076Z", "updatedAt": "2023-10-11T21:22:04.076Z", "environment": {"sys": {"id": "master", "type": "Link", "linkType": "Environment"}}, "revision": 1, "locale": "en-US"}, "fields": {"title": "WorkLife S6 - 385 x 1052x", "description": "", "file": {"url": "//images.ctfassets.net/ab732rbh901q/3toTf7QHhGfKy59Qcx69dk/9b35ad517313f033f03f743e335118d5/WorkLife_S6_-_385_x_1052x.png", "details": {"size": 26833, "image": {"width": 770, "height": 210}}, "fileName": "WorkLife S6 - 385 x 1052x.png", "contentType": "image/png"}}}, "imageXLBreakpoint": {"metadata": {"tags": []}, "sys": {"space": {"sys": {"type": "Link", "linkType": "Space", "id": "ab732rbh901q"}}, "id": "3M4ANHhDr3Nwsd385dCSFK", "type": "Asset", "createdAt": "2023-10-11T21:22:04.078Z", "updatedAt": "2023-10-11T21:22:04.078Z", "environment": {"sys": {"id": "master", "type": "Link", "linkType": "Environment"}}, "revision": 1, "locale": "en-US"}, "fields": {"title": "WorkLife S6 - 472 x 1072x", "description": "", "file": {"url": "//images.ctfassets.net/ab732rbh901q/3M4ANHhDr3Nwsd385dCSFK/9d558d98ba8cbc09bb163323934ccd10/WorkLife_S6_-_472_x_1072x.png", "details": {"size": 27426, "image": {"width": 944, "height": 202}}, "fileName": "WorkLife S6 - 472 x 1072x.png", "contentType": "image/png"}}}}, "status": "published"}, "preview": false, "shortenedUrl": "https://go.ted.com/LLWr", "action": null, "videoData": {"__typename": "Video", "id": "46456", "slug": "sergio_feferovich_la_musica_de_las_ideas", "title": "La m\u00fasica de las ideas", "socialTitle": "La m\u00fasica de las ideas", "presenterDisplayName": "Sergio Feferovich", "internalLanguageCode": "en", "commentsEnabled": false, "commentsLoggedInOnly": false, "recordedOn": "2012-10-24", "curatorApproved": true, "viewedCount": 377692, "duration": 1220, "publishedAt": "2019-08-19T20:32:09Z", "topics": {"__typename": "TopicConnection", "nodes": [{"__typename": "Topic", "id": "37", "name": "entertainment", "slug": "entertainment"}, {"__typename": "Topic", "id": "40", "name": "music", "slug": "music"}, {"__typename": "Topic", "id": "41", "name": "performance", "slug": "performance"}, {"__typename": "Topic", "id": "68", "name": "creativity", "slug": "creativity"}, {"__typename": "Topic", "id": "69", "name": "art", "slug": "art"}]}, "talkExtras": {"__typename": "TalkExtras", "recommendations": [], "takeAction": [], "learnModules": []}, "primaryImageSet": [{"__typename": "PhotoSize", "url": "https://talkstar-photos.s3.amazonaws.com/uploads/0168193e-ab59-49df-8ef8-9247011c0cf6/Sergio+Feferovich.jpeg", "aspectRatioName": "16x9"}], "relatedVideos": [{"__typename": "Video", "slug": "vincent_moon_and_nana_vasconcelos_hidden_music_rituals_around_the_world", "id": "2131"}, {"__typename": "Video", "slug": "joey_alexander_an_11_year_old_prodigy_performs_old_school_jazz", "id": "2273"}, {"__typename": "Video", "slug": "scott_rickard_the_beautiful_math_behind_the_world_s_ugliest_music", "id": "1331"}, {"__typename": "Video", "slug": "david_byrne_how_architecture_helped_music_evolve", "id": "883"}, {"__typename": "Video", "slug": "michael_tilson_thomas_music_and_emotion_through_time", "id": "1440"}, {"__typename": "Video", "slug": "tod_machover_dan_ellsey_inventing_instruments_that_unlock_new_music", "id": "246"}], "customContentDetails": {"__typename": "CustomContentDetails", "partnerName": null}, "speakers": {"__typename": "AcmeSpeakerConnection", "nodes": [{"__typename": "AcmeSpeaker", "photoUrl": "", "firstname": "Sergio", "middlename": "", "lastname": "Feferovich", "description": "Conductor, pianist", "isLive": false, "title": "", "whatOthersSay": "", "whoTheyAre": "Dr. Sergio Feferovich holds a PhD in Music from John Hopkins University and a Master\u2019s degree in Orchestral Conducting from the university's Peabody Institute. He has conducted numerous choirs and orchestras in his home country of Argentina and beyond.", "whyListen": "", "slug": "sergio_feferovich"}]}, "description": "From Mozart's operatic arias of love to Jo\u00e3o Gilberto's bossa nova tunes of play, music is full of ideas worth spreading. In a fun and symphonic talk, acclaimed orchestra conductor Sergio Feferovich showcases the ways in which music can introduce ideas that reach far beyond the notes composed.", "socialDescription": "From Mozart's operatic arias of love to Jo\u00e3o Gilberto's bossa nova tunes of play, music is full of ideas worth spreading. In a fun and symphonic talk, acclaimed orchestra conductor Sergio Feferovich showcases the ways in which music can introduce ideas that reach far beyond the notes composed.", "partnerName": null, "playerData": "{\"id\":\"46456\",\"mediaIdentifier\":\"\",\"mediaProjectVersionIdentifier\":null,\"duration\":1220,\"languages\":[{\"languageName\":\"English\",\"endonym\":\"English\",\"languageCode\":\"en\",\"ianaCode\":\"en\",\"isRtl\":false},{\"languageName\":\"Portuguese\",\"endonym\":\"Portugu\u00eas de Portugal\",\"languageCode\":\"pt\",\"ianaCode\":\"pt\",\"isRtl\":false},{\"languageName\":\"Spanish\",\"endonym\":\"Espa\u00f1ol\",\"languageCode\":\"es\",\"ianaCode\":\"es\",\"isRtl\":false}],\"nativeLanguage\":\"en\",\"isSubtitleRequired\":false,\"resources\":{\"h264\":null,\"hls\":{\"adUrl\":\"https://pubads.g.doubleclick.net/gampad/ads?ciu_szs=300x250%2C512x288%2C120x60%2C320x50%2C6x7%2C6x8&correlator=%5Bcorrelator%5D&cust_params=event%3DTEDxRiodelaPlata%26id%3D46456%26tag%3Dart%2Ccreativity%2Cmusic%2Centertainment%2Cperformance%26talk%3Dsergio_feferovich_la_musica_de_las_ideas%26year%3D2012&env=vp&gdfp_req=1&impl=s&iu=%2F5641%2Fmobile%2Fios%2Fweb&output=xml_vast2&sz=640x360&unviewed_position_start=1&url=%5Breferrer%5D\",\"maiTargeting\":{\"id\":\"46456\",\"talk\":\"sergio_feferovich_la_musica_de_las_ideas\",\"tag\":\"art,creativity,music,entertainment,performance\",\"year\":\"2012\",\"event\":\"TEDxRiodelaPlata\"},\"stream\":null,\"metadata\":null}},\"targeting\":{\"id\":\"46456\",\"talk\":\"sergio_feferovich_la_musica_de_las_ideas\",\"tag\":\"art,creativity,music,entertainment,performance\",\"year\":\"2012\",\"event\":\"TEDxRiodelaPlata\"},\"canonical\":\"https://www.ted.com/talks/sergio_feferovich_la_musica_de_las_ideas\",\"name\":\"Sergio Feferovich: La m\u00fasica de las ideas\",\"title\":\"La m\u00fasica de las ideas\",\"speaker\":\"Sergio Feferovich\",\"thumb\":\"https://pi.tedcdn.com/r/talkstar-photos.s3.amazonaws.com/uploads/0168193e-ab59-49df-8ef8-9247011c0cf6/Sergio+Feferovich.jpeg?quality=89&w=600\",\"slug\":\"sergio_feferovich_la_musica_de_las_ideas\",\"event\":\"TEDxRiodelaPlata\",\"published\":1566246729,\"external\":{\"service\":\"YouTube\",\"code\":\"1wA2sl9IRcE\",\"duration\":1220.0,\"start_time\":0.0}}", "videoContext": "TEDxRiodelaPlata", "audioInternalLanguageCode": "en", "language": "en", "hasTranslations": true, "featured": false, "type": {"__typename": "TypeOfVideo", "id": "2", "name": "TEDx Talk"}}, "transcriptData": {"translation": {"__typename": "Translation", "id": "50329", "language": {"__typename": "Language", "id": "35", "endonym": "English", "englishName": "English", "internalLanguageCode": "en", "rtl": false}, "reviewer": {"__typename": "User", "id": "228180", "uri": "/profiles/228293", "avatar": {"__typename": "UserAvatar", "url": "https://s3.amazonaws.com/ted.conferences.profiles/00/00/0d/a9/95/895381.jpeg", "generatedUrl": "https://avatars.ted.com/v1/avatar/1345425139.svg"}, "name": {"__typename": "UserName", "full": "Sebastian Betti"}}, "translator": {"__typename": "User", "id": "4600998", "uri": "/profiles/4602733", "avatar": {"__typename": "UserAvatar", "url": "https://s3.amazonaws.com/ted.conferences.profiles/00/00/08/44/c2/541890.jpeg", "generatedUrl": "https://avatars.ted.com/v1/avatar/1436268051.svg"}, "name": {"__typename": "UserName", "full": "Raquel Rivas"}}, "paragraphs": [{"__typename": "Paragraph", "cues": [{"__typename": "Cue", "text": "Ideas worth spreading.", "time": 19852}, {"__typename": "Cue", "text": "When is an idea worth spreading?", "time": 23552}, {"__typename": "Cue", "text": "Is an idea good by itself?", "time": 26643}, {"__typename": "Cue", "text": "Can inappropriate context ruin an idea?", "time": 31528}, {"__typename": "Cue", "text": "Can a simple idea work", "time": 37288}, {"__typename": "Cue", "text": "within a very motivating context?", "time": 38676}, {"__typename": "Cue", "text": "Here I have a piano.", "time": 44774}, {"__typename": "Cue", "text": "And within music there are ideas.", "time": 47773}, {"__typename": "Cue", "text": "For example, if I play this,", "time": 49492}, {"__typename": "Cue", "text": "(Music)", "time": 51198}, {"__typename": "Cue", "text": "I can see ladies in the audience sighing", "time": 57479}, {"__typename": "Cue", "text": "because it is a wonderful idea.", "time": 60645}, {"__typename": "Cue", "text": "Who doesn't know Beethoven's 'Fur Elise'?", "time": 62371}, {"__typename": "Cue", "text": "Now, this idea needs a context.", "time": 65078}, {"__typename": "Cue", "text": "On the piano, the context\nis played by the left hand.", "time": 67401}, {"__typename": "Cue", "text": "The left hand plays this.", "time": 71016}, {"__typename": "Cue", "text": "(Music)", "time": 72831}, {"__typename": "Cue", "text": "Perfect. I have an idea\nand a context.", "time": 78027}, {"__typename": "Cue", "text": "Then, I just have to put them together,", "time": 80735}, {"__typename": "Cue", "text": "So it sounds like this.", "time": 83257}, {"__typename": "Cue", "text": "(Music)", "time": 84654}, {"__typename": "Cue", "text": "Something is wrong.", "time": 92754}, {"__typename": "Cue", "text": "You may notice that this", "time": 94466}, {"__typename": "Cue", "text": "(Music)", "time": 95917}, {"__typename": "Cue", "text": "can't be Beethoven.", "time": 96578}, {"__typename": "Cue", "text": "It is wrong because the melody, the idea,", "time": 98984}, {"__typename": "Cue", "text": "(Music)", "time": 101728}, {"__typename": "Cue", "text": "is in one tone\nbut the context is in another.", "time": 103001}, {"__typename": "Cue", "text": "(Music)", "time": 106163}, {"__typename": "Cue", "text": "This happens often in real life too.\nYou feel like you have an idea", "time": 107037}, {"__typename": "Cue", "text": "but the context is in\na different tone.", "time": 109705}, {"__typename": "Cue", "text": "If I take the context and do this.", "time": 112342}, {"__typename": "Cue", "text": "(Music)", "time": 114001}, {"__typename": "Cue", "text": "I change it", "time": 115614}, {"__typename": "Cue", "text": "(Music)", "time": 116705}, {"__typename": "Cue", "text": "and I put it in the same frequency\nand tone as the idea.", "time": 118454}, {"__typename": "Cue", "text": "(Music)", "time": 122976}, {"__typename": "Cue", "text": "Then, everything starts to work.", "time": 127033}, {"__typename": "Cue", "text": "But this example is easy,", "time": 131298}, {"__typename": "Cue", "text": "if the idea is as wonderful\nas Beethoven's idea.", "time": 133741}, {"__typename": "Cue", "text": "But imagine that a boy says to me", "time": 137756}, {"__typename": "Cue", "text": "\"Hi Fefe\" --", "time": 141986}, {"__typename": "Cue", "text": "they call me Fefe, I don't know why.", "time": 143445}, {"__typename": "Cue", "text": "\"Hi Fefe. I want to be a composer\nand I have composed my first piece,", "time": 145266}, {"__typename": "Cue", "text": "this is the idea I have.", "time": 150879}, {"__typename": "Cue", "text": "(Music)", "time": 152171}, {"__typename": "Cue", "text": "And I say, \"Yes, yes, very nice.\"", "time": 160149}, {"__typename": "Cue", "text": "It reminds me of the story of the father\nthat makes his son study violin", "time": 163006}, {"__typename": "Cue", "text": "and takes him to a friend and says:", "time": 168015}, {"__typename": "Cue", "text": "\"Look, Juan, listen how my son\nplays the violin.\"", "time": 169626}, {"__typename": "Cue", "text": "The son plays the violin...\nvery badly. (Laughter)", "time": 172081}, {"__typename": "Cue", "text": "And he says: \"what do you think\nabout the execution?\"", "time": 175105}, {"__typename": "Cue", "text": "\"Well, executing him is excessive,\na couple of slaps would be enough.\"", "time": 177686}, {"__typename": "Cue", "text": "(Laughter)", "time": 181497}, {"__typename": "Cue", "text": "But, that wasn't the case.", "time": 184096}, {"__typename": "Cue", "text": "(Applause)", "time": 185995}, {"__typename": "Cue", "text": "Without going that far\nit occurred to me to say", "time": 189603}, {"__typename": "Cue", "text": "\"If this is your idea... (Music)", "time": 192624}, {"__typename": "Cue", "text": "you'd better do something else, son\".", "time": 195001}, {"__typename": "Cue", "text": "But, if that child was Fr\u00e9d\u00e9ric Chopin,", "time": 197058}, {"__typename": "Cue", "text": "a Polish composer\nof the nineteenth century,", "time": 199665}, {"__typename": "Cue", "text": "he could have written", "time": 201689}, {"__typename": "Cue", "text": "(Music)", "time": 202821}, {"__typename": "Cue", "text": "And with this very simple idea,\nwhich could deserve a slap,", "time": 213597}, {"__typename": "Cue", "text": "he composed a prelude,", "time": 216432}, {"__typename": "Cue", "text": "\"Prelude in E Minor\"", "time": 217638}, {"__typename": "Cue", "text": "a wonderful piece that is played\nin piano recitals everywhere.", "time": 218923}, {"__typename": "Cue", "text": "So we had these two examples.", "time": 223855}, {"__typename": "Cue", "text": "But, what happens if I say\nI have an idea,", "time": 225365}, {"__typename": "Cue", "text": "I'm going to be a composer,\nand this is my idea.", "time": 228149}, {"__typename": "Cue", "text": "(Note)", "time": 231733}, {"__typename": "Cue", "text": "Ha! (Laughter)\nWhat do you think?", "time": 233101}, {"__typename": "Cue", "text": "I hear laughs. But be careful,\nbecause laughter can be disrespectful", "time": 237580}, {"__typename": "Cue", "text": "because this note, (Note)", "time": 241047}, {"__typename": "Cue", "text": "this idea, can be repeated.", "time": 242814}, {"__typename": "Cue", "text": "(Two notes)", "time": 244774}, {"__typename": "Cue", "text": "Ha! It starts to sound different.", "time": 245897}, {"__typename": "Cue", "text": "I can even change the rhythm --", "time": 248329}, {"__typename": "Cue", "text": "rhythm is the duration of a note.", "time": 250431}, {"__typename": "Cue", "text": "I can do this. (Notes)", "time": 252321}, {"__typename": "Cue", "text": "If I am Tom Jobim, for example,", "time": 255609}, {"__typename": "Cue", "text": "a Brazilian composer, I can do this.", "time": 257541}, {"__typename": "Cue", "text": "(Music)", "time": 259611}, {"__typename": "Cue", "text": "This is \"One-note Samba.\"", "time": 269850}, {"__typename": "Cue", "text": "you may have noticed\nwhy it's called that.", "time": 271624}, {"__typename": "Cue", "text": "(Laughter)", "time": 274201}, {"__typename": "Cue", "text": "Now, this one-note samba", "time": 275665}, {"__typename": "Cue", "text": "makes the most of\nthe idea of one idea", "time": 278488}, {"__typename": "Cue", "text": "- the redundancy is intended -", "time": 282769}, {"__typename": "Cue", "text": "reduced to the expression\nof a single note.", "time": 284522}, {"__typename": "Cue", "text": "But, speaking of reducing ideas,", "time": 288446}, {"__typename": "Cue", "text": "there is one example\nwhich is pretty average.", "time": 290982}, {"__typename": "Cue", "text": "The most interesting example\nof synthesizing an idea", "time": 294534}, {"__typename": "Cue", "text": "was invented by John Cage,", "time": 298883}, {"__typename": "Cue", "text": "who composed a piece for piano", "time": 300809}, {"__typename": "Cue", "text": "called \"4 Minutes 33 Seconds.\"", "time": 304009}, {"__typename": "Cue", "text": "And in this case,\nto be faithful to the piece,", "time": 308674}, {"__typename": "Cue", "text": "I'm going to use a score\nif you don't mind.", "time": 311011}, {"__typename": "Cue", "text": "And I'm going to play just a fragment,", "time": 318001}, {"__typename": "Cue", "text": "because, you know, time is...", "time": 320104}, {"__typename": "Cue", "text": "(Laughter)", "time": 332620}, {"__typename": "Cue", "text": "We could keep going,\nbut I think you get the picture.", "time": 344505}, {"__typename": "Cue", "text": "(Applause)", "time": 348915}, {"__typename": "Cue", "text": "There must be, perhaps,\na more intense version somewhere,", "time": 353958}, {"__typename": "Cue", "text": "but it's less interesting\nfor an orchestra.", "time": 356782}, {"__typename": "Cue", "text": "You can listen to it if you want.\n(Laughter)", "time": 359514}, {"__typename": "Cue", "text": "And, speaking of ideas in music,", "time": 361485}, {"__typename": "Cue", "text": "I'm thinking, to what extent\nis an idea our own?", "time": 363328}, {"__typename": "Cue", "text": "Really our own.", "time": 367361}, {"__typename": "Cue", "text": "And when does an idea\nbegin to be shared?", "time": 370746}, {"__typename": "Cue", "text": "There is an aria by Mozart\nfrom the Don Giovanni opera", "time": 374037}, {"__typename": "Cue", "text": "--Don Juan--", "time": 376718}, {"__typename": "Cue", "text": "it's the story of a man\nwho is a womanizer and even though", "time": 377707}, {"__typename": "Cue", "text": "he seduces one woman after another,\nhe is never satisfied.", "time": 384494}, {"__typename": "Cue", "text": "This aria is \"L\u00e0 ci darem la mano\"", "time": 388103}, {"__typename": "Cue", "text": "where Don Giovanni sings\nto one of his lovers", "time": 390544}, {"__typename": "Cue", "text": "\"come I'll give you my hand,\nwe will go far away,", "time": 393683}, {"__typename": "Cue", "text": "you will accept this, etc.\"", "time": 396799}, {"__typename": "Cue", "text": "It sounds like this.", "time": 398906}, {"__typename": "Cue", "text": "(Music)", "time": 400547}, {"__typename": "Cue", "text": "L\u00e0 ci darem la mano,", "time": 401563}, {"__typename": "Cue", "text": "L\u00e0 mi dirai di s\u00ec:", "time": 405771}, {"__typename": "Cue", "text": "Vedi, non \u00e8 lontano...", "time": 409896}, {"__typename": "Cue", "text": "'El Reino del Reves' (Spanish\nnursery rhyme) (Laughter)", "time": 413022}, {"__typename": "Cue", "text": "It's very similar to that\nnursery rhyme! (Applause)", "time": 418001}, {"__typename": "Cue", "text": "Now, this example is less clear\nbecause we can think", "time": 422968}, {"__typename": "Cue", "text": "has this idea been done before?\nIs it new?", "time": 426433}, {"__typename": "Cue", "text": "A composer, we could say,", "time": 429764}, {"__typename": "Cue", "text": "found things before they were lost.", "time": 433646}, {"__typename": "Cue", "text": "This is a euphemism for stealing.", "time": 438275}, {"__typename": "Cue", "text": "This composer,", "time": 440473}, {"__typename": "Cue", "text": "(Laughter)", "time": 441861}, {"__typename": "Cue", "text": "let's say he is a popular composer,", "time": 442582}, {"__typename": "Cue", "text": "ran out of inspiration.", "time": 444943}, {"__typename": "Cue", "text": "He didn't show inspiration\nin other work either,", "time": 448075}, {"__typename": "Cue", "text": "but in this case", "time": 450435}, {"__typename": "Cue", "text": "he wanted inspiration,\nso he searched his old jukebox", "time": 451678}, {"__typename": "Cue", "text": "and his old vinyl discs.", "time": 455099}, {"__typename": "Cue", "text": "To the youngest among you,", "time": 457344}, {"__typename": "Cue", "text": "back then, we didn't have pendrives\nor CDs. We had vinyl discs, you know.", "time": 458646}, {"__typename": "Cue", "text": "And he played this vinyl disc in search\nfor inspiration; it was a tango.", "time": 463027}, {"__typename": "Cue", "text": "(Music)", "time": 467799}, {"__typename": "Cue", "text": "It was broken. It didn't work.\nSo he threw it away", "time": 475550}, {"__typename": "Cue", "text": "and looked for another\nand he read on the cover", "time": 477950}, {"__typename": "Cue", "text": "\"Muzio Clementi\",", "time": 481719}, {"__typename": "Cue", "text": "\"Sonatina en sol mayor\".", "time": 483452}, {"__typename": "Cue", "text": "Who knows Muzio Clementi?", "time": 487281}, {"__typename": "Cue", "text": "Well, Muzio Clementi\nwas a great composer.", "time": 490374}, {"__typename": "Cue", "text": "He was much less famous\nthan his genius deserved.", "time": 492605}, {"__typename": "Cue", "text": "He was Beethoven's teacher,\namong others.", "time": 496478}, {"__typename": "Cue", "text": "And in the third movement of that sonata", "time": 498642}, {"__typename": "Cue", "text": "- the one that the composer\nwas using for inspiration -", "time": 502212}, {"__typename": "Cue", "text": "he wrote this.", "time": 504968}, {"__typename": "Cue", "text": "(Music)", "time": 505879}, {"__typename": "Cue", "text": "But the composer thought:", "time": 513514}, {"__typename": "Cue", "text": "\"I don't like this.\nSomething is wrong.\"", "time": 515405}, {"__typename": "Cue", "text": "\"And, what is this button for?\"", "time": 517371}, {"__typename": "Cue", "text": "There was a button,\nmetronome, that had", "time": 518881}, {"__typename": "Cue", "text": "three numbers: 33, 45 and 78.", "time": 520457}, {"__typename": "Cue", "text": "(Laughter)", "time": 524618}, {"__typename": "Cue", "text": "It was set in 78.", "time": 525826}, {"__typename": "Cue", "text": "So he thought: \"This is wrong.\nLet me put it in 45.\"", "time": 527678}, {"__typename": "Cue", "text": "(Music)", "time": 530422}, {"__typename": "Cue", "text": "\"No. I don't like this either.\"", "time": 535136}, {"__typename": "Cue", "text": "\"Let's see what happens in 33.\"", "time": 536994}, {"__typename": "Cue", "text": "(Music)", "time": 540103}, {"__typename": "Cue", "text": "I have a hit!", "time": 544007}, {"__typename": "Cue", "text": "(Music)", "time": 546210}, {"__typename": "Cue", "text": "And who, who's going to complain?", "time": 551022}, {"__typename": "Cue", "text": "(Applause)", "time": 553778}, {"__typename": "Cue", "text": "Muzio Clementi died\nmore than 300 years ago.", "time": 559006}, {"__typename": "Cue", "text": "Who is going to remember?\nWho knows?", "time": 563346}, {"__typename": "Cue", "text": "Only the idiots\nwho go and listen to TED talks", "time": 565731}, {"__typename": "Cue", "text": "and investigate those things.", "time": 568539}, {"__typename": "Cue", "text": "(Laughter)", "time": 569992}, {"__typename": "Cue", "text": "Now, in an unprecedented effort", "time": 571016}, {"__typename": "Cue", "text": "because the sonatina doesn't end there.", "time": 573023}, {"__typename": "Cue", "text": "(Music)", "time": 575249}, {"__typename": "Cue", "text": "It goes on...", "time": 576304}, {"__typename": "Cue", "text": "(Music)", "time": 577547}, {"__typename": "Cue", "text": "This is the next hit.", "time": 581000}, {"__typename": "Cue", "text": "It says:", "time": 583001}, {"__typename": "Cue", "text": "\"When I'm feeling happy,\nwhen I'm feeling honey.", "time": 583937}, {"__typename": "Cue", "text": "All I have to do\nis steal another tune\".", "time": 589241}, {"__typename": "Cue", "text": "(Laughter)", "time": 593645}, {"__typename": "Cue", "text": "Done! Now I just have to publish it.", "time": 594700}, {"__typename": "Cue", "text": "And we all get rich.", "time": 596475}, {"__typename": "Cue", "text": "Now, I once heard --", "time": 598797}, {"__typename": "Cue", "text": "whilst we're on the topic of ideas --", "time": 601996}, {"__typename": "Cue", "text": "I heard Alejandro Dolina say", "time": 604740}, {"__typename": "Cue", "text": "that the genius was not the guy\nwho invented the first phone.", "time": 607769}, {"__typename": "Cue", "text": "The genius was the one\nwho invented the second phone.", "time": 612409}, {"__typename": "Cue", "text": "Because one phone alone\nis not worth it.", "time": 615044}, {"__typename": "Cue", "text": "(Laughter)", "time": 618321}, {"__typename": "Cue", "text": "In the case of ideas it's the same.", "time": 620118}, {"__typename": "Cue", "text": "Some composers have great ideas.", "time": 623208}, {"__typename": "Cue", "text": "But the really brilliant composers", "time": 625805}, {"__typename": "Cue", "text": "are those who find\nthe second phone for each idea.", "time": 628995}, {"__typename": "Cue", "text": "And that is why one of my favorite\nand most popular", "time": 632919}, {"__typename": "Cue", "text": "composers is undoubtedly Mozart.", "time": 636411}, {"__typename": "Cue", "text": "We were listening to him before", "time": 638009}, {"__typename": "Cue", "text": "- I mean, I quoted him before -", "time": 639933}, {"__typename": "Cue", "text": "in \"L\u00e0 ci darem la mano.\"", "time": 642574}, {"__typename": "Cue", "text": "He composed a Sonata in C Major", "time": 644366}, {"__typename": "Cue", "text": "a piano sonata that\ncould be used to tell a story.", "time": 646478}, {"__typename": "Cue", "text": "And the idea sounds like this:", "time": 652447}, {"__typename": "Cue", "text": "it begins when we are born", "time": 656172}, {"__typename": "Cue", "text": "(Music)", "time": 657775}, {"__typename": "Cue", "text": "and then we go to kindergarten.", "time": 660164}, {"__typename": "Cue", "text": "(Music)", "time": 662105}, {"__typename": "Cue", "text": "We play", "time": 664806}, {"__typename": "Cue", "text": "with other children", "time": 667141}, {"__typename": "Cue", "text": "with our friends.", "time": 669446}, {"__typename": "Cue", "text": "Then we grow older,", "time": 674898}, {"__typename": "Cue", "text": "we finish school", "time": 676200}, {"__typename": "Cue", "text": "- elementary school -", "time": 678541}, {"__typename": "Cue", "text": "and then, all of a sudden\nthere she is.", "time": 680578}, {"__typename": "Cue", "text": "Ahh!", "time": 684719}, {"__typename": "Cue", "text": "When I first met my wife, I asked her", "time": 687402}, {"__typename": "Cue", "text": "- I just met her and,\nplaying the piano, I asked -", "time": 689470}, {"__typename": "Cue", "text": "\"Do you like music?\"", "time": 691963}, {"__typename": "Cue", "text": "She said: \"Yes, but never mind,\nkeep playing.\"", "time": 693135}, {"__typename": "Cue", "text": "(Laughter)", "time": 695471}, {"__typename": "Cue", "text": "She appears...", "time": 697145}, {"__typename": "Cue", "text": "(Music)", "time": 698485}, {"__typename": "Cue", "text": "All is well.\n\"Would you like to go out?\" \"Yes!\"", "time": 704385}, {"__typename": "Cue", "text": "\"To have a cup of coffee...\nhave a great time together.\"", "time": 707499}, {"__typename": "Cue", "text": "There we are, very happy. And this\nis the time when you have to ask:", "time": 712737}, {"__typename": "Cue", "text": "\"Do you like me being a musician?\"", "time": 717493}, {"__typename": "Cue", "text": "\"Yes!\"", "time": 719779}, {"__typename": "Cue", "text": "\"Do you like me being a bohemian,\nnot interested in things?\"", "time": 721047}, {"__typename": "Cue", "text": "\"Love it\"", "time": 723878}, {"__typename": "Cue", "text": "\"Sign it!\"", "time": 724585}, {"__typename": "Cue", "text": "(Laughter)", "time": 725789}, {"__typename": "Cue", "text": "Because I want to have that\non the record,", "time": 727048}, {"__typename": "Cue", "text": "because that...", "time": 729247}, {"__typename": "Cue", "text": "(Music)", "time": 730114}, {"__typename": "Cue", "text": "is about to end.", "time": 732936}, {"__typename": "Cue", "text": "\"How nice. How about living together?", "time": 735028}, {"__typename": "Cue", "text": "How about getting married?\" \"OK!\"", "time": 739066}, {"__typename": "Cue", "text": "\"Let's fix a date.\"", "time": 744494}, {"__typename": "Cue", "text": "(Music)", "time": 746149}, {"__typename": "Cue", "text": "And then the marriage begins...", "time": 750163}, {"__typename": "Cue", "text": "Oh!\n(Laughter)", "time": 752030}, {"__typename": "Cue", "text": "This is Mozart, eh?", "time": 755196}, {"__typename": "Cue", "text": "It's Mozart.", "time": 757268}, {"__typename": "Cue", "text": "(Applause)", "time": 758057}, {"__typename": "Cue", "text": "This is Mozart.", "time": 759894}, {"__typename": "Cue", "text": "(Music)", "time": 761077}, {"__typename": "Cue", "text": "The kids.", "time": 762563}, {"__typename": "Cue", "text": "Work.", "time": 763977}, {"__typename": "Cue", "text": "The house.", "time": 767857}, {"__typename": "Cue", "text": "(Music)", "time": 769404}, {"__typename": "Cue", "text": "And then...\nIs this the life I want?", "time": 772697}, {"__typename": "Cue", "text": "(Music)", "time": 776339}, {"__typename": "Cue", "text": "All you want is to go back\nto the beginning", "time": 778868}, {"__typename": "Cue", "text": "when you were a child", "time": 783119}, {"__typename": "Cue", "text": "(Music)", "time": 784879}, {"__typename": "Cue", "text": "Ah! There we are --", "time": 787495}, {"__typename": "Cue", "text": "This is like the beginning.\nBut the beginning was like this.", "time": 789988}, {"__typename": "Cue", "text": "(Music)", "time": 792729}, {"__typename": "Cue", "text": "And now it's like this.", "time": 793524}, {"__typename": "Cue", "text": "(Music)", "time": 795146}, {"__typename": "Cue", "text": "It's not the same... it's alike\nbut it's not the same.", "time": 796493}, {"__typename": "Cue", "text": "Now we have gained a few kilos,", "time": 798677}, {"__typename": "Cue", "text": "lost some hair...", "time": 802812}, {"__typename": "Cue", "text": "The other day I was sitting\nand my son said to me:", "time": 804897}, {"__typename": "Cue", "text": "\"Dad, you are getting bald.\"", "time": 807001}, {"__typename": "Cue", "text": "(Laughter)", "time": 809009}, {"__typename": "Cue", "text": "And I said: \"It's true.\" because now\nwe're going through the same thing.", "time": 811587}, {"__typename": "Cue", "text": "(Music)", "time": 815195}, {"__typename": "Cue", "text": "But we still enjoy it, right?", "time": 819058}, {"__typename": "Cue", "text": "And you are looking forward\n- the kids are grown up -", "time": 823014}, {"__typename": "Cue", "text": "to meeting that person\nthat you once knew...", "time": 825861}, {"__typename": "Cue", "text": "(Music)", "time": 830318}, {"__typename": "Cue", "text": "and you feel that you have found her.", "time": 836024}, {"__typename": "Cue", "text": "Ah!", "time": 842015}, {"__typename": "Cue", "text": "But, how was she before?", "time": 843518}, {"__typename": "Cue", "text": "(Music)", "time": 845050}, {"__typename": "Cue", "text": "And now she is...", "time": 846846}, {"__typename": "Cue", "text": "(Music)", "time": 847951}, {"__typename": "Cue", "text": "That's okay, it's close enough.", "time": 850801}, {"__typename": "Cue", "text": "(Laughter)", "time": 852246}, {"__typename": "Cue", "text": "What can you do?", "time": 853821}, {"__typename": "Cue", "text": "I've gained some weight too...", "time": 855295}, {"__typename": "Cue", "text": "(Laughter)", "time": 857196}, {"__typename": "Cue", "text": "\"Will we play? Will we go out?\n\"No, let's stay here.\"", "time": 859598}, {"__typename": "Cue", "text": "(Laughter)", "time": 863473}, {"__typename": "Cue", "text": "\"Dr. House isn't that bad\"", "time": 865336}, {"__typename": "Cue", "text": "(Laughter)", "time": 867235}, {"__typename": "Cue", "text": "\"And Util\u00edsima is so useful,", "time": 871054}, {"__typename": "Cue", "text": "it has recipes and everything!", "time": 874010}, {"__typename": "Cue", "text": "(Laughter)", "time": 875577}, {"__typename": "Cue", "text": "\"I like that roast beef with potatoes.\"", "time": 876991}, {"__typename": "Cue", "text": "And I will end there.", "time": 883278}, {"__typename": "Cue", "text": "(Applause)", "time": 884697}, {"__typename": "Cue", "text": "And now that we're talking about\nideas in music that go together,", "time": 896990}, {"__typename": "Cue", "text": "and since we all trust each other\nand won't tell anyone else", "time": 902087}, {"__typename": "Cue", "text": "I propose that together,", "time": 906507}, {"__typename": "Cue", "text": "we write a hit.", "time": 908456}, {"__typename": "Cue", "text": "Let's write a song that could sell", "time": 910340}, {"__typename": "Cue", "text": "and then we'll distribute the profits.\nNobody will know.", "time": 914412}, {"__typename": "Cue", "text": "But, to make a hit we need an idea.", "time": 918441}, {"__typename": "Cue", "text": "An idea could have three notes.", "time": 922564}, {"__typename": "Cue", "text": "There are 7 musical notes, right?", "time": 924874}, {"__typename": "Cue", "text": "C, D, E, F, G, A, B.", "time": 926820}, {"__typename": "Cue", "text": "There are some alterations: flat,\nsharp... But that doesn't matter.", "time": 929236}, {"__typename": "Cue", "text": "Together, let's create an idea\nwith three notes.", "time": 932400}, {"__typename": "Cue", "text": "Now, over here, give me a note.", "time": 936606}, {"__typename": "Cue", "text": "C, D, E, F, G, A, B?\nA note?", "time": 938812}, {"__typename": "Cue", "text": "F.", "time": 940190}, {"__typename": "Cue", "text": "F, perfect, F.", "time": 941189}, {"__typename": "Cue", "text": "F.", "time": 942503}, {"__typename": "Cue", "text": "F, what a good idea!", "time": 945940}, {"__typename": "Cue", "text": "People will say, F!\nCome on! F!", "time": 948353}, {"__typename": "Cue", "text": "F.", "time": 953371}, {"__typename": "Cue", "text": "And you over there?\nGive me a note.", "time": 954207}, {"__typename": "Cue", "text": "OK. OK. Let's say B.", "time": 958022}, {"__typename": "Cue", "text": "Flat? Sharp?", "time": 959887}, {"__typename": "Cue", "text": "Just B?", "time": 961773}, {"__typename": "Cue", "text": "Flat. B flat.", "time": 962856}, {"__typename": "Cue", "text": "It's getting there, eh? Getting there.", "time": 967592}, {"__typename": "Cue", "text": "Let's see over there, another note?", "time": 969578}, {"__typename": "Cue", "text": "D.", "time": 971251}, {"__typename": "Cue", "text": "Flat? Sharp?", "time": 972416}, {"__typename": "Cue", "text": "D flat.", "time": 974540}, {"__typename": "Cue", "text": "Now. We already have the idea!", "time": 979253}, {"__typename": "Cue", "text": "And in order to make it a hit,\n- because that's all we want -", "time": 982059}, {"__typename": "Cue", "text": "to be able to sell it very well,", "time": 984988}, {"__typename": "Cue", "text": "and make a lot of money, we have\nto make something popular.", "time": 987389}, {"__typename": "Cue", "text": "A \"bolero\" for example,\nwhat do you think?", "time": 992122}, {"__typename": "Cue", "text": "(Music)", "time": 994171}, {"__typename": "Cue", "text": "Ah! But - sorry, I have to make\nthe cameramen move a bit -", "time": 1008285}, {"__typename": "Cue", "text": "You know what?", "time": 1011965}, {"__typename": "Cue", "text": "If this is a bolero, we need lyrics.", "time": 1014320}, {"__typename": "Cue", "text": "Boleros have words.", "time": 1017002}, {"__typename": "Cue", "text": "This idea needs words.", "time": 1020159}, {"__typename": "Cue", "text": "What word can we use? Let's see.", "time": 1023012}, {"__typename": "Cue", "text": "You know what?\nI'm going to tell you", "time": 1027589}, {"__typename": "Cue", "text": "something that I learned\nafter many years of studying.", "time": 1030365}, {"__typename": "Cue", "text": "If the idea has three notes\nthe text needs three syllables.", "time": 1033141}, {"__typename": "Cue", "text": "If you tell me \"TEDxR\u00edodelaPlata\"", "time": 1037557}, {"__typename": "Cue", "text": "there is no way I could do it.\nLet's see, Max?", "time": 1040079}, {"__typename": "Cue", "text": "Could you give me a word?", "time": 1042944}, {"__typename": "Cue", "text": "What?", "time": 1045038}, {"__typename": "Cue", "text": "Give me a word for a bolero?", "time": 1045943}, {"__typename": "Cue", "text": "We are creating a hit here.", "time": 1047309}, {"__typename": "Cue", "text": "\"So you were...\"", "time": 1048797}, {"__typename": "Cue", "text": "I was playing the piano", "time": 1049786}, {"__typename": "Cue", "text": "Give me something to make a hit.", "time": 1050939}, {"__typename": "Cue", "text": "Three notes, one word.", "time": 1053053}, {"__typename": "Cue", "text": "This is a bolero.", "time": 1054325}, {"__typename": "Cue", "text": "\"Ambition\"", "time": 1055581}, {"__typename": "Cue", "text": "That's it: Ambition!", "time": 1058036}, {"__typename": "Cue", "text": "Ambition to love you, to have you\nambition for whatever you want!", "time": 1060065}, {"__typename": "Cue", "text": "So, let's do something.\nYou remember the idea, right?", "time": 1063541}, {"__typename": "Cue", "text": "(Music)", "time": 1066157}, {"__typename": "Cue", "text": "Let's see if we can sing this.", "time": 1067249}, {"__typename": "Cue", "text": "Please sing \"Ambition\"\nin these three notes.", "time": 1068830}, {"__typename": "Cue", "text": "(Singing) Ambition.", "time": 1072822}, {"__typename": "Cue", "text": "(Laughter)", "time": 1075745}, {"__typename": "Cue", "text": "We have a problem here.\nLet's see.", "time": 1077653}, {"__typename": "Cue", "text": "Breathe properly, let's do it again.", "time": 1079613}, {"__typename": "Cue", "text": "(Singing) Ambition.", "time": 1082042}, {"__typename": "Cue", "text": "Again.", "time": 1086045}, {"__typename": "Cue", "text": "(Singing) Ambition.", "time": 1087174}, {"__typename": "Cue", "text": "Great! I'm going to fill in the gaps", "time": 1089065}, {"__typename": "Cue", "text": "but when the theme comes back", "time": 1091236}, {"__typename": "Cue", "text": "we'll sing it loudly. Ready?", "time": 1093027}, {"__typename": "Cue", "text": "Let's go. We begin with Ambition", "time": 1096057}, {"__typename": "Cue", "text": "(Singing) Ambition.", "time": 1098689}, {"__typename": "Cue", "text": "Stop, stop, stop.", "time": 1102492}, {"__typename": "Cue", "text": "Now go!", "time": 1104698}, {"__typename": "Cue", "text": "Right now.", "time": 1106983}, {"__typename": "Cue", "text": "(Singing) Ambition.", "time": 1108111}, {"__typename": "Cue", "text": "Louder, louder.", "time": 1111967}, {"__typename": "Cue", "text": "(Singing) Ambition.", "time": 1112979}, {"__typename": "Cue", "text": "And...", "time": 1116790}, {"__typename": "Cue", "text": "(Singing) Ambition.", "time": 1117586}, {"__typename": "Cue", "text": "And to make it a real hit,\nit needs to end with an ovation", "time": 1119816}, {"__typename": "Cue", "text": "because, people will think...", "time": 1123498}, {"__typename": "Cue", "text": "We're recording this live, right now,\nso you can buy it at the exit,", "time": 1125181}, {"__typename": "Cue", "text": "you can digitalize it.", "time": 1128682}, {"__typename": "Cue", "text": "So, over here.", "time": 1129889}, {"__typename": "Cue", "text": "Let's do a rehearsal of the ovation", "time": 1131628}, {"__typename": "Cue", "text": "like this.", "time": 1136839}, {"__typename": "Cue", "text": "(Applause)", "time": 1137837}, {"__typename": "Cue", "text": "Everyone over there.", "time": 1139078}, {"__typename": "Cue", "text": "(Applause)", "time": 1140013}, {"__typename": "Cue", "text": "More, more, right at the back!", "time": 1141301}, {"__typename": "Cue", "text": "Great! Very good!", "time": 1144369}, {"__typename": "Cue", "text": "You'll do that.", "time": 1145595}, {"__typename": "Cue", "text": "And you begin to sing,", "time": 1147140}, {"__typename": "Cue", "text": "as the applause grows\nyou stop singing", "time": 1148744}, {"__typename": "Cue", "text": "and start clapping.", "time": 1151763}, {"__typename": "Cue", "text": "And that's how we finish\nthis TEDxR\u00edodelaPlata.", "time": 1153383}, {"__typename": "Cue", "text": "Let's see how it goes, eh?", "time": 1157808}, {"__typename": "Cue", "text": "Everybody sing, okay?", "time": 1159082}, {"__typename": "Cue", "text": "When I give the signal,\nwe give an ovation.", "time": 1160203}, {"__typename": "Cue", "text": "We are recording this,\nso, be serious.", "time": 1163032}, {"__typename": "Cue", "text": "And... three...", "time": 1165032}, {"__typename": "Cue", "text": "(Singing) Ambition.", "time": 1166408}, {"__typename": "Cue", "text": "(Singing) Ambition.", "time": 1171395}, {"__typename": "Cue", "text": "(Singing) Ambition.\n(Applause)", "time": 1176032}, {"__typename": "Cue", "text": "(Singing) Ambition.\n(Applause)", "time": 1180574}, {"__typename": "Cue", "text": "(Music)", "time": 1185188}, {"__typename": "Cue", "text": "(Ovation)", "time": 1193091}]}]}, "video": {"__typename": "Video", "id": "46456", "talkExtras": {"__typename": "TalkExtras", "footnotes": []}}}, "commentsEnabled": false, "commentsLoggedInOnly": false}, "language": "en", "messages": {}, "responseCode": 200, "__N_SSP": true}